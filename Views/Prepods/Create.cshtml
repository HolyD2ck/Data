@model Data.Models.Prepod

@{
    ViewData["Title"] = "Create";
}

<h1 class="t">Внести</h1>

<h4>Преподаватель:</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Фамилия" class="control-label"></label>
                <input asp-for="Фамилия" class="form-control" />
                <span asp-validation-for="Фамилия" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Имя" class="control-label"></label>
                <input asp-for="Имя" class="form-control" />
                <span asp-validation-for="Имя" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Отчество" class="control-label"></label>
                <input asp-for="Отчество" class="form-control" />
                <span asp-validation-for="Отчество" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Куратор_Группы" class="control-label"></label>
                <input asp-for="Куратор_Группы" class="form-control" />
                <span asp-validation-for="Куратор_Группы" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Профессия" class="control-label"></label>
                <input asp-for="Профессия" class="form-control" />
                <span asp-validation-for="Профессия" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="День_Рождения" class="control-label"></label>
                <input asp-for="День_Рождения" class="form-control" />
                <span asp-validation-for="День_Рождения" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Номер_Кабинета" class="control-label"></label>
                <input asp-for="Номер_Кабинета" class="form-control" />
                <span asp-validation-for="Номер_Кабинета" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Зарплата" class="control-label"></label>
                <input asp-for="Зарплата" class="form-control" />
                <span asp-validation-for="Зарплата" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Стаж" class="control-label"></label>
                <input asp-for="Стаж" class="form-control" />
                <span asp-validation-for="Стаж" class="text-danger"></span>
            </div>
            <br>
            <div class="form-group">
                <input type="submit" value="Внести" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<br>
<div>
    <a asp-action="Index">На страницу просмотра</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

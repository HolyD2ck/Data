@model IEnumerable<Data.Models.View>

@{
    ViewData["Title"] = "Index";
}

<h1 class="t">Младше</h1>
<br>
<table class="table">
    <thead>
        <tr>
           
            <th>
                @Html.DisplayNameFor(model => model.id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Фамилия)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Имя)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Отчество)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Рост)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.День_Рождения)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Группа)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Специальность)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Стипендия)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Возраст)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
            
        <tr>
           <td>
                @Html.DisplayFor(modelItem => item.id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Фамилия)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Имя)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Отчество)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Рост)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.День_Рождения)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Группа)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Специальность)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Стипендия)
            </td>
            <td>
                 @Html.DisplayFor(modelItem => item.Возраст)
            </td>
        </tr>
}
    </tbody>
</table>
